// Inspired by Catnap
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    "display": {
        "separator": "",
        "key": {
            "width": 12
        }
    },
    "modules": [
        {
            // draw borders first to make colors of left and right border consistant
            "key": " user",
            "type": "title",
            "format": "{user-name}",
            "keyColor": "31"
        },
        {
            "key": "{icon} distro",
            "type": "os",
            "format": "{pretty-name}",
            "keyColor": "32"
        },
        {
            "type": "wm",
            "key": " wm",
            "format": "{pretty-name}",
            "keyColor": "33"
        },
        {
            "type": "de",
            "key": " de",
            "format": "{pretty-name}",
            "keyColor": "34"
        },
        {
            "key": " term",
            "type": "terminal",
            "format": "{pretty-name}",
            "keyColor": "35"
        },
        {
            "key": " shell",
            "type": "shell",
            "format": "{pretty-name}",
            "keyColor": "36"
        },
        {
            "key": "󰌢 host",
            "type": "host",
            "keyColor": "31"

        },
        {
            "key": "󰍛 cpu",
            "type": "cpu",
            "showPeCoreCount": true,
            "keyColor": "32"
        },
        {
            "key": "󰉉 disk",
            "type": "disk",
            "folders": "/",
            "keyColor": "33"
        },
        {
            "key": " memory",
            "type": "memory",
            "keyColor": "34"
        },
        {
            "key": "󰅐 uptime",
            "type": "uptime",
            "keyColor": "35"
        },
        {
            "key": " colors",
            "type": "colors",
            "symbol": "circle",
            "keyColor": "39"
        }
    ]
}
